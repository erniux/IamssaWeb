      <ul>
        <% servicio.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <%= form.submit  'Guardar Servicio', class: "btn alert-primary form-control" %><br><br>
