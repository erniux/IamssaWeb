      Cantidad:&nbsp;<%= form.text_field :cantidad, class: "form-control" %>
    </div>
    <div class="col-md-4">
      Tipo Servicio:&nbsp;<%= form.text_field :tipo_servicio, class: "form-control" %>
    </div>
  </div>

